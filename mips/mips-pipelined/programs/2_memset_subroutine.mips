            addi    $a0, $zero, 8         # 001000 00000 00100 0000000000001000
            addi    $a1, $zero, 0xbe      # 001000 00000 00101 0000000010111110
            sll     $a1, $a1, 8           # 000000 00000 00101 00101 01000 000000
            addi    $a1, $zero, 0xef      # 001000 00000 00101 0000000011101111
            addi    $a2, $zero, 16        # 001000 00000 00110 0000000000010000
            jal     memset                # 000011 00000000000000000001000000

            add     $a0, $zero, $v0       # 000000 00000 00010 00100 00000 100000
            addi    $a1, $zero, 0         # 001000 00000 00101 0000000000000000
            addi    $a2, $zero, 8         # 001000 00000 00110 0000000000001000
            jal     memset                # 000011 00000000000000000001000000

            add     $a0, $zero, $v0       # 000000 00000 00010 00100 00000 100000
            addi    $a1, $zero, 0xffff    # 001000 00000 00101 1111111111111111
            sll     $a1, $a1, 16          # 000000 00000 00101 00101 10000 000000
            addi    $a1, $zero, 0xffff    # 001000 00000 00101 1111111111111111
            addi    $a2, $zero, 16        # 001000 00000 00110 0000000000010000
            jal     memset                # 000011 00000000000000000001000000

# ================================================================
# a0 - starting address
# a1 - value to put in memory
# a2 - how many bytes
# v0 - address stopped at
memset:
            addi    $t0, $zero, 0         # 001000 00000 01000 0000000000000000
ms_loop:    sw      $a1, 0($a0)           # 101011 00100 00101 0000000000000000
            addi    $a0, $a0, 4           # 001000 00100 00100 0000000000000100
            addi    $t0, $t0, 1           # 001000 01000 01000 0000000000000001
            bne     $t0, $a2, ms_loop     # 000101 01000 00110 1111111111110100
            jr      $ra                   # 000000 11111 00000 00000 00000 001000

